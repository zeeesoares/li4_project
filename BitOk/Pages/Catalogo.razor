@page "/catalogo"

<PageTitle>Catálogo de Produtos</PageTitle>

<div class="container mx-auto mt-8">
    <div class="flex flex-col lg:flex-row gap-6">
        <!-- Barra lateral para filtros -->
        <div class="w-full lg:w-1/4 bg-gray-100 p-4 rounded-lg">
            <h4 class="text-lg font-semibold mb-4">Filtros</h4>

            <div class="mb-4">
                <h5 class="text-sm font-medium">Categorias</h5>
                <div class="flex flex-col gap-2">
                    <label class="flex items-center space-x-2">
                        <input type="checkbox" class="form-checkbox text-blue-600" />
                        <span>Gaming</span>
                    </label>
                    <label class="flex items-center space-x-2">
                        <input type="checkbox" class="form-checkbox text-blue-600" />
                        <span>Office</span>
                    </label>
                    <label class="flex items-center space-x-2">
                        <input type="checkbox" class="form-checkbox text-blue-600" />
                        <span>Liquid</span>
                    </label>
                </div>
            </div>

            <div class="mb-4">
                <h5 class="text-sm font-medium">Preço</h5>
                <input type="range" class="w-full" min="0" max="5000" step="100" @bind="priceFilter" />
                <p class="text-sm mt-1">Até: €@priceFilter</p>
            </div>

            <div class="mb-4">
                <h5 class="text-sm font-medium">Processador</h5>
                <div class="flex flex-col gap-2">
                    <label class="flex items-center space-x-2">
                        <input type="radio" name="processor" class="form-radio text-blue-600" />
                        <span>AMD</span>
                    </label>
                    <label class="flex items-center space-x-2">
                        <input type="radio" name="processor" class="form-radio text-blue-600" />
                        <span>Intel</span>
                    </label>
                </div>
            </div>

            <div>
                <h5 class="text-sm font-medium">Gráfica</h5>
                <div class="flex flex-col gap-2">
                    <label class="flex items-center space-x-2">
                        <input type="checkbox" class="form-checkbox text-blue-600" />
                        <span>4090</span>
                    </label>
                    <label class="flex items-center space-x-2">
                        <input type="checkbox" class="form-checkbox text-blue-600" />
                        <span>4080</span>
                    </label>
                    <label class="flex items-center space-x-2">
                        <input type="checkbox" class="form-checkbox text-blue-600" />
                        <span>AMD Radeon</span>
                    </label>
                </div>
            </div>
        </div>

        <!-- Grade de produtos -->
        <div class="w-full lg:w-3/4">
            <div class="flex justify-between items-center mb-6">
                <h4 class="text-xl font-semibold">Produtos</h4>
                <div class="flex space-x-2">
                    <button class="px-4 py-2 bg-gray-200 rounded-md hover:bg-gray-300">New</button>
                    <button class="px-4 py-2 bg-gray-200 rounded-md hover:bg-gray-300">Preço ↑</button>
                    <button class="px-4 py-2 bg-gray-200 rounded-md hover:bg-gray-300">Preço ↓</button>
                </div>
            </div>

            <div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 gap-6">
                @foreach (var product in products)
                {
                    <div class="bg-white shadow-lg rounded-lg overflow-hidden">
                        <img src="@product.ImageUrl" alt="@product.Name" class="w-full h-80 object-cover" />
                        <div class="p-4">
                            <h5 class="text-lg font-semibold">@product.Name</h5>
                            <p class="text-sm text-gray-600 mt-2">@product.Description</p>
                            <p class="text-lg font-bold text-blue-600 mt-4">€@product.Price</p>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>

@code {
    // Simulação de filtro por preço
    private int priceFilter = 5000;

    // Lista de produtos simulada
    private List<Product> products = new List<Product>
    {
        new Product { Name = "Computador King Mod Gaming", Description = "PC AMD Ryzen 5600G 16GB DDR4", Price = 449.90m, ImageUrl = "https://img.globaldata.pt/products/KM4LTD-R51TN46WW2.jpg?auto=compress%2Cformat&fit=fill&fill-color=fff&q=70&fill=solid&w=727&h=727" },
        new Product { Name = "Computador King Mod Liquid", Description = "PC i5 RTX 4080 32GB DDR5", Price = 2999.90m, ImageUrl = "data:image/jpeg;base64," },
        new Product { Name = "Computador King Mod Office", Description = "PC i7 16GB DDR4", Price = 1499.90m, ImageUrl = "data:image/jpeg;base64," },
        new Product { Name = "Computador King Mod Creator", Description = "PC i9 RTX 4070 64GB DDR5", Price = 3999.90m, ImageUrl = "data:image/jpeg;base64," }
    };

    // Classe para produtos
    public class Product
    {
        public string Name { get; set; }
        public string Description { get; set; }
        public decimal Price { get; set; }
        public string ImageUrl { get; set; }
    }
}